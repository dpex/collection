    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "yii2 collection",
        "collection"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Igor Chepurnoi",
            "email": "chepurnoi.igor@gmail.com"
        }
